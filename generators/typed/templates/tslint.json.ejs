{
  "extends": "tslint-config-typings",
  "rules": {
    "no-unused-variable": false,
    "interface-name": [
      true,
      "always-prefix"
    ],
    "variable-name": [
      true,
      "check-format"
    ],
    "typedef-whitespace": [
      true,
      {
        "index-signature": "nospace",
        "parameter": "nospace",
        "property-declaration": "nospace",
        "variable-declaration": "nospace"
      }
    ]
  }
}
