{
  "private": true,
  "name": "drupal-typings-<%= moduleNameDash %>",
  "version": "1.0.0",
  "description": "TypeScript type definitions for <%= moduleName %> module",
  "keywords": [
    "drupal",
    "<%= moduleName %>",
    "typescript",
    "typings"
  ],
  "license": "GPL-2.0+",
  "repository": {
    "type": "git",
    "url": "https://git.drupal.org/project/typings_<%= moduleName %>.git"
  },
  "devDependencies": {
    "tslint": "^3.13.0",
    "typescript": "^1.8.10",
    "typings": "^1.3.0"
  },
  "scripts": {
    "get-started": "typings install && npm run tsc",
    "tsc": "tsc --project './source'",
    "test": "npm run lint && npm run tsc",
    "lint": "npm run lint-tslint",
    "lint-tslint": "tslint --format verbose --config './tslint.json' './source/**/*.ts'"
  }
}
